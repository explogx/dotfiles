Host *
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa

Host github.com
    User git
    Hostname github.com
    PreferredAuthentications publickey
    AddKeysToAgent yes
    IdentityFile ~/.ssh/github

Host ubuntu-vm
    User ubuntu
    Hostname localhost
    Port 2222
    PreferredAuthentications publickey
    AddKeysToAgent yes
    IdentityFile ~/.ssh/ubuntu-vm
